{
  "name": "url-shortner",
  "version": "1.0.0",
  "description": "A url shortner app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Godswilldev/url-shortner.git"
  },
  "author": "Godswilldev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Godswilldev/url-shortner/issues"
  },
  "homepage": "https://github.com/Godswilldev/url-shortner#readme",
  "dependencies": {
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5"
  }
}
